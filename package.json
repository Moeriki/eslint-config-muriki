    "eslint-plugin-node": "3.0.5",
    "eslint-plugin-react": "6.8.0",
    "husky": "0.12.0",
    "jest": "18.1.0",
    "lint-staged": "3.2.5"
  },
  "peerDependencies": {
    "eslint": "3.12",
    "eslint-plugin-import": "2.2",
    "eslint-plugin-jasmine": "2",
    "eslint-plugin-jsx-a11y": "3",
    "eslint-plugin-node": "3",
    "eslint-plugin-react": "6"
  },
  "jest": {
    "testRegex": "/__tests__/test\\.js$"
